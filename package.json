{
  "name": "flow-agent-kit",
  "version": "1.0.0",
  "description": "Kit de desenvolvimento para criar agentes de IA que interagem com a blockchain Flow",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "node --loader ts-node/esm src/index.ts",
