  },
  "homepage": "https://github.com/Flow-Brasil/flow-agent-kit#readme",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@langchain/core": "^0.1.48",
    "@onflow/fcl": "^1.10.0",
    "@onflow/types": "^1.2.1",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
